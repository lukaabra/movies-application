{
  "name": "dickeys-assignment",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "nodemon server.js",
    "debug": "nodemon --inspect server.js",
    "migrate": "npx knex migrate:latest --knexfile ./db/knexfile.js",
