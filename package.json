  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^10.0.0",
    "express": "~4.16.1",
    "find-config": "^1.0.0",
    "http-errors": "^1.6.3",
    "knex": "^0.95.7",
    "nodemon": "^2.0.11",
    "pg": "^8.6.0"
  },
  "devDependencies": {
    "jest": "^27.0.6"
  }
}
